{
    "name": "steam-backbone-storefront",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "jquery": "latest",
        "jqueryui": "latest",
        "underscore": "latest",
        "momentjs": "latest",
        "moment-timezone": "latest",
        "backbone": "latest",
        "backbone-helper": "latest",
        "bootstrap": "latest",
        "font-awesome": "latest",
        "bootstrap-sass-official": "latest",
        "dustjs-linkedin": "latest",
        "dustjs-linkedin-helpers": "latest",
        "socket.io-client": "latest",
        "requirejs": "latest",
        "requirejs-text": "latest"
    },
    "exportsOverride": {
        "bootstrap": {
            "": [
                "./dist/css/bootstrap.css",
                "./dist/css/bootstrap.css.map",
                "./dist/js/bootstrap.js",
                "./dist/fonts/glyphicons-halflings-regular.eot",
                "./dist/fonts/glyphicons-halflings-regular.svg",
                "./dist/fonts/glyphicons-halflings-regular.ttf",
                "./dist/fonts/glyphicons-halflings-regular.woff"
            ]
        },
        "bootstrap-sass-official": {
            "bootstrap": "vendor/assets/stylesheets/bootstrap/*.scss",
            "bootstrap/mixins": "vendor/assets/stylesheets/bootstrap/mixins/*.scss"
        },
        "dustjs-linkedin": {
            "": "dist/dust-full.js"
        },
        "dustjs-linkedin-helpers": {
            "": "dist/dust-helpers.js"
        },
        "font-awesome": {
            "fonts": "fonts",
            "": "css/font-awesome.css"
        },
        "jqueryui": {
            "themes": "themes",
            "":"ui/jquery-ui.js"
        },
        "moment-timezone": {
            "": "moment-timezone.js"
        },
        "momentjs": {
            "": "moment.js"
        },
        "requirejs-text": {
            "": "text.js"
        }
    }
}